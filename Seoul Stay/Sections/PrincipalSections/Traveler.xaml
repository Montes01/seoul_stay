<UserControl x:Class="Seoul_Stay.Sections.PrincipalSections.Traveler"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Seoul_Stay.Sections.PrincipalSections"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Background="White"
             >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="q" TextChanged="q_TextChanged" Grid.Row="0" Margin="20"/>
        <DataGrid x:Name="Services" Grid.Row="1"/>
    </Grid>


</UserControl>
